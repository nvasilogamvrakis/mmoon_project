                          rdfs:subClassOf owl:Thing ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty [ owl:inverseOf <http://purl.org/co/item>
                                                           ] ;
                                            owl:someValuesFrom <http://purl.org/co/Bag>
                                          ] ;
