#################################################################
#    Object Properties
#################################################################

###  http://mmoon.org/core/associatedToLanguage
<http://mmoon.org/core/associatedToLanguage> rdf:type owl:ObjectProperty ;
                                             rdfs:range <http://mmoon.org/core/Language> ;
                                             <http://mmoon.org/core/definition> "States that a morpheme inventory, morph or word is associated to a specific langauge."@en ;
                                             rdfs:label "associated to language"@en .


###  http://mmoon.org/core/attachedTo
<http://mmoon.org/core/attachedTo> rdf:type owl:ObjectProperty ;
                                   rdfs:domain <http://mmoon.org/core/Affix> ;
                                   rdfs:range [ rdf:type owl:Class ;
                                                owl:unionOf ( <http://mmoon.org/core/Root>
                                                              <http://mmoon.org/core/Stem>
                                                            )
                                              ] ;
                                   <http://mmoon.org/core/definition> "States that an affix is attached to some stem or root."@en ;
                                   rdfs:label "attached to"@en .


###  http://mmoon.org/core/attachedToRoot
<http://mmoon.org/core/attachedToRoot> rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf <http://mmoon.org/core/attachedTo> ;
                                       rdfs:domain <http://mmoon.org/core/Affix> ;
                                       rdfs:range <http://mmoon.org/core/Root> ;
                                       <http://mmoon.org/core/definition> "States that an affix is attached to some root."@en ;
                                       rdfs:label "attached to root"@en .


###  http://mmoon.org/core/attachedToStem
<http://mmoon.org/core/attachedToStem> rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf <http://mmoon.org/core/attachedTo> ;
                                       rdfs:domain <http://mmoon.org/core/Affix> ;
                                       rdfs:range <http://mmoon.org/core/Stem> ;
                                       <http://mmoon.org/core/definition> "States that an affix is attached to some stem."@en ;
                                       rdfs:label "attached to stem"@en .


###  http://mmoon.org/core/belongsTo
<http://mmoon.org/core/belongsTo> rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                  owl:inverseOf <http://mmoon.org/core/consistsOfMorph> ;
                                  rdfs:domain <http://mmoon.org/core/Morph> ;
                                  rdfs:range [ rdf:type owl:Class ;
                                               owl:unionOf ( <http://mmoon.org/core/Root>
                                                             <http://mmoon.org/core/Stem>
                                                             <http://mmoon.org/core/Word>
                                                           )
                                             ] ;
                                  <http://mmoon.org/core/definition> "States that a morph belongs to some word, stem or root."@en ;
                                  rdfs:label "belongs to"@en .


###  http://mmoon.org/core/belongsToLexicalEntry
<http://mmoon.org/core/belongsToLexicalEntry> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <http://mmoon.org/core/hasWordform> ;
                                              rdfs:domain <http://mmoon.org/core/Wordform> ;
                                              rdfs:range <http://mmoon.org/core/LexicalEntry> ;
                                              <http://mmoon.org/core/definition> "States that a word-form resource belongs to a lexical entry resource."@en ;
                                              rdfs:label "belongs to lexical entry"@en .


###  http://mmoon.org/core/compoundingRelation
<http://mmoon.org/core/compoundingRelation> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf <http://mmoon.org/core/hasMorphologicalRelationship> ;
                                            rdfs:domain <http://mmoon.org/core/CompoundWord> ;
                                            rdfs:range <http://mmoon.org/core/Compounding> ;
                                            <http://mmoon.org/core/definition> "States which kind of compounding relationship applies to a compound word."@en ;
                                            rdfs:label "compounding relation"@en .


###  http://mmoon.org/core/consistsOfAffix
<http://mmoon.org/core/consistsOfAffix> rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf <http://mmoon.org/core/consistsOfMorph> ;
                                        rdfs:range <http://mmoon.org/core/Affix> ;
                                        <http://mmoon.org/core/definition> "States that a word, stem or root consists of some affix."@en ;
                                        rdfs:label "consists of affix"@en .


###  http://mmoon.org/core/consistsOfMorph
<http://mmoon.org/core/consistsOfMorph> rdf:type owl:ObjectProperty ;
                                        rdfs:domain [ rdf:type owl:Class ;
                                                      owl:unionOf ( <http://mmoon.org/core/Root>
                                                                    <http://mmoon.org/core/Stem>
                                                                    <http://mmoon.org/core/Word>
                                                                  )
                                                    ] ;
                                        rdfs:range <http://mmoon.org/core/Morph> ;
                                        <http://mmoon.org/core/definition> "States that a word, stem or root consists of some morph."@en ;
                                        rdfs:label "consists of morph"@en .


###  http://mmoon.org/core/consistsOfRoot
<http://mmoon.org/core/consistsOfRoot> rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf <http://mmoon.org/core/consistsOfMorph> ;
                                       rdfs:range <http://mmoon.org/core/Root> ;
                                       <http://mmoon.org/core/definition> "States that a word, stem or root consists of some root."@en ;
                                       rdfs:label "consists of root"@en .


###  http://mmoon.org/core/consistsOfStem
<http://mmoon.org/core/consistsOfStem> rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf <http://mmoon.org/core/consistsOfMorph> ;
                                       rdfs:domain [ rdf:type owl:Class ;
                                                     owl:unionOf ( <http://mmoon.org/core/Stem>
                                                                   <http://mmoon.org/core/Word>
                                                                 )
                                                   ] ;
                                       rdfs:range <http://mmoon.org/core/Stem> ;
                                       <http://mmoon.org/core/definition> "States that a word or stem consists of some stem."@en ;
                                       rdfs:label "consists of stem"@en .


###  http://mmoon.org/core/consistsOfWord
<http://mmoon.org/core/consistsOfWord> rdf:type owl:ObjectProperty ;
                                       rdfs:domain [ rdf:type owl:Class ;
                                                     owl:unionOf ( <http://mmoon.org/core/AnalyticWordform>
                                                                   <http://mmoon.org/core/CompoundWord>
                                                                 )
                                                   ] ;
                                       rdfs:range <http://mmoon.org/core/Word> ;
                                       <http://mmoon.org/core/definition> "States that an analytic word-form (such as \"more big\" being the comparative form of the adjective \"big\") or compound word consists of other word resources."@en ;
                                       rdfs:label "consists of word"@en .


###  http://mmoon.org/core/contextualInflectionalMeaning
<http://mmoon.org/core/contextualInflectionalMeaning> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://mmoon.org/core/inflectionalMeaning> ;
                                                      rdfs:range <http://mmoon.org/core/LinguisticCategory> ;
                                                      <http://mmoon.org/core/definition> """States that the meaning \"comprises values (of linguistic categories) which are assigned [...] because of the syntactic context in which it (the word or morph) appears\".
[Haspelmath and Sims: Understanding morphology. 2002:100]"""@en ;
                                                      <http://mmoon.org/core/example> "\"klein-e Frau\" 'small-NOM.SG.F woman.NOM.SG.F' - contextual inflection (noun agreement marking)"@de ;
                                                      rdfs:label "contextual inflectional meaning"@en .


###  http://mmoon.org/core/correspondsToMorpheme
<http://mmoon.org/core/correspondsToMorpheme> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf <http://mmoon.org/core/hasRealization> ;
                                              rdfs:domain <http://mmoon.org/core/Morph> ;
                                              rdfs:range <http://mmoon.org/core/Morpheme> ;
                                              <http://mmoon.org/core/definition> "States that a morph corresponds to a morpheme."@en ;
                                              rdfs:label "corresponds to morpheme"@en .


###  http://mmoon.org/core/derivationalMeaning
<http://mmoon.org/core/derivationalMeaning> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf <http://mmoon.org/core/hasMeaning> ;
                                            rdfs:range <http://mmoon.org/core/DerivationalMeaning> ;
                                            owl:propertyChainAxiom ( :consists_of_DerSuf
                                                                     <http://mmoon.org/core/derivationalMeaning>
                                                                   ) ;
                                            <http://mmoon.org/core/definition> "States that a morph, morpheme or word resource has some derivational meaning."@en ;
                                            rdfs:label "derivational meaning"@en .


###  http://mmoon.org/core/derivationalRelation
<http://mmoon.org/core/derivationalRelation> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <http://mmoon.org/core/hasMorphologicalRelationship> ;
                                             rdfs:domain <http://mmoon.org/core/DerivedWord> ;
                                             rdfs:range <http://mmoon.org/core/Derivation> ;
                                             <http://mmoon.org/core/definition> "States which kind of derivational  relationship applies to a derived word."@en ;
                                             <http://mmoon.org/core/usageNote> "This property is also used to state that a simple lexeme which cannot be further segmented with respect to word-formation has the morphological relationship mmoon:NoWordFormation."@en ;
                                             rdfs:label "derivational relation"@en .


###  http://mmoon.org/core/forLanguage
<http://mmoon.org/core/forLanguage> rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf <http://mmoon.org/core/associatedToLanguage> ;
                                    rdfs:domain <http://mmoon.org/core/MorphemeInventory> ;
                                    <http://mmoon.org/core/definition> "States that a MMoOn morpheme inventory is specified for a certain language."@en ;
                                    rdfs:label "for language"@en .


###  http://mmoon.org/core/grammaticalMeaning
<http://mmoon.org/core/grammaticalMeaning> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf <http://mmoon.org/core/hasMeaning> ;
                                           rdfs:range [ rdf:type owl:Class ;
                                                        owl:unionOf ( <http://mmoon.org/core/LinguisticCategory>
                                                                      <http://mmoon.org/core/WordclassAffiliation>
                                                                    )
                                                      ] ;
                                           owl:propertyChainAxiom ( :consists_of_DerSuf
                                                                    <http://mmoon.org/core/grammaticalMeaning>
                                                                  ) ,
                                                                  ( :consists_of_InfSuf
                                                                    <http://mmoon.org/core/grammaticalMeaning>
                                                                  ) ,
                                                                  ( :consists_of_StemOfAPrefStem
                                                                    <http://mmoon.org/core/grammaticalMeaning>
                                                                  ) ,
                                                                  ( :consists_of_StemOfAWord
                                                                    <http://mmoon.org/core/grammaticalMeaning>
                                                                  ) ;
                                           <http://mmoon.org/core/definition> "States that a morph, morpheme or word resource has some grammatical meaning."@en ;
                                           rdfs:label "grammatical meaning"@en .


###  http://mmoon.org/core/hasAbstractIdentity
<http://mmoon.org/core/hasAbstractIdentity> rdf:type owl:ObjectProperty ;
                                            rdfs:domain [ rdf:type owl:Class ;
                                                          owl:unionOf ( <http://mmoon.org/core/Meaning>
                                                                        <http://mmoon.org/core/Morpheme>
                                                                      )
                                                        ] ;
                                            rdfs:range <http://mmoon.org/core/MorphemicGloss> ;
                                            <http://mmoon.org/core/definition> "States that a morpheme or meaning resource is assigned to one (ore more) morphemic gloss(es)."@en ;
                                            rdfs:label "has abstract identity"@en .


###  http://mmoon.org/core/hasMeaning
<http://mmoon.org/core/hasMeaning> rdf:type owl:ObjectProperty ;
                                   rdfs:domain [ rdf:type owl:Class ;
                                                 owl:unionOf ( <http://mmoon.org/core/Morph>
                                                               <http://mmoon.org/core/Morpheme>
                                                               <http://mmoon.org/core/Word>
                                                             )
                                               ] ;
                                   rdfs:range <http://mmoon.org/core/Meaning> ;
                                   <http://mmoon.org/core/definition> "States that a morph, morpheme or word resource has some meaning."@en ;
                                   rdfs:label "has meaning"@en .


###  http://mmoon.org/core/hasMorphemicEntry
<http://mmoon.org/core/hasMorphemicEntry> rdf:type owl:ObjectProperty ;
                                          rdfs:domain <http://mmoon.org/core/MorphemeInventory> ;
                                          rdfs:range [ rdf:type owl:Class ;
                                                       owl:unionOf ( <http://mmoon.org/core/Morph>
                                                                     <http://mmoon.org/core/Morpheme>
                                                                   )
                                                     ] ;
                                          <http://mmoon.org/core/definition> "States that a morpheme inventory resource has morphemes as morphemic entries, thus building the intension of the morpheme inventory."@en ;
                                          rdfs:label "has morphemic entry"@en .


###  http://mmoon.org/core/hasMorphologicalRelationship
<http://mmoon.org/core/hasMorphologicalRelationship> rdf:type owl:ObjectProperty ;
                                                     rdfs:domain <http://mmoon.org/core/Word> ;
                                                     rdfs:range <http://mmoon.org/core/MorphologicalRelationship> ;
                                                     <http://mmoon.org/core/definition> "Specifies the morphological relationship of a word."@en ;
                                                     rdfs:label "has morphological relationship"@en .


###  http://mmoon.org/core/hasRealization
<http://mmoon.org/core/hasRealization> rdf:type owl:ObjectProperty ;
                                       rdfs:domain <http://mmoon.org/core/Morpheme> ;
                                       rdfs:range <http://mmoon.org/core/Morph> ;
                                       <http://mmoon.org/core/definition> "States that a morpheme is realized by a morph."@en ;
                                       rdfs:label "has realization"@en .


###  http://mmoon.org/core/hasRepresentation
<http://mmoon.org/core/hasRepresentation> rdf:type owl:ObjectProperty ;
                                          rdfs:domain [ rdf:type owl:Class ;
                                                        owl:unionOf ( <http://mmoon.org/core/Morph>
                                                                      <http://mmoon.org/core/Word>
                                                                    )
                                                      ] ;
                                          rdfs:range <http://mmoon.org/core/Representation> ;
                                          <http://mmoon.org/core/definition> "States that a word or morph has some linguistic representation."@en ;
                                          rdfs:label "has representation"@en .


###  http://mmoon.org/core/hasSense
<http://mmoon.org/core/hasSense> rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf <http://mmoon.org/core/hasMeaning> ;
                                 rdfs:range <http://mmoon.org/core/Sense> ;
                                 owl:propertyChainAxiom ( :consists_of_StemOfAWord
                                                          <http://mmoon.org/core/hasSense>
                                                        ) ;
                                 <http://mmoon.org/core/definition> "States that a morph, morpheme or word resource has some sense."@en ;
                                 rdfs:label "has sense"@en .


###  http://mmoon.org/core/hasWordclassAffiliation
<http://mmoon.org/core/hasWordclassAffiliation> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://mmoon.org/core/hasMeaning> ;
                                                rdfs:range <http://mmoon.org/core/WordclassAffiliation> ;
                                                <http://mmoon.org/core/definition> "States that a morph, morpheme or word resource has a wordclass affiliation."@en ;
                                                rdfs:label "has wordclass affiliation"@en .


###  http://mmoon.org/core/hasWordform
<http://mmoon.org/core/hasWordform> rdf:type owl:ObjectProperty ;
                                    rdfs:domain <http://mmoon.org/core/LexicalEntry> ;
                                    rdfs:range <http://mmoon.org/core/Wordform> ;
                                    <http://mmoon.org/core/definition> "States that a lexical entry resource has an associated word-form resource."@en ;
                                    rdfs:label "has wordform"@en .


###  http://mmoon.org/core/inflectionalMeaning
<http://mmoon.org/core/inflectionalMeaning> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf <http://mmoon.org/core/hasMeaning> ;
                                            rdfs:range <http://mmoon.org/core/LinguisticCategory> ;
                                            <http://mmoon.org/core/definition> "States that a morph, morpheme or word resource has some inflectional meaning."@en ;
                                            rdfs:label "inflectional meaning"@en .


###  http://mmoon.org/core/inflectionalRelation
<http://mmoon.org/core/inflectionalRelation> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <http://mmoon.org/core/hasMorphologicalRelationship> ;
                                             rdfs:domain <http://mmoon.org/core/Word> ;
                                             rdfs:range <http://mmoon.org/core/Inflection> ;
                                             <http://mmoon.org/core/definition> "States which kind of inflectional relationship applies to a word."@en ;
                                             rdfs:label "inflectional relation"@en .


###  http://mmoon.org/core/inherentInflectionalMeaning
<http://mmoon.org/core/inherentInflectionalMeaning> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf <http://mmoon.org/core/inflectionalMeaning> ;
                                                    rdfs:range <http://mmoon.org/core/LinguisticCategory> ;
                                                    <http://mmoon.org/core/definition> """States that the meaning \"comprises features (= linguistic categories) that are relevant to the syntax but convey a certain amount of independent information\". E.g. the tense/aspect values for verbs or the number or gender values for nouns.
[Haspelmath and Sims: Understanding morphology. 2002:100]"""@de ;
                                                    <http://mmoon.org/core/example> "\"klein-er-e Frau\" 'small-COMPR-NOM.SG.F woman.NOM.SG.F' - inherent inflection for degree of comparison"@de ;
                                                    rdfs:label "inherent inflectionalm eaning"@en .


###  http://mmoon.org/core/isAllomorphTo
<http://mmoon.org/core/isAllomorphTo> rdf:type owl:ObjectProperty ,
                                               owl:SymmetricProperty ;
